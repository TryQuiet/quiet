{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "74",
          "node": "10"
        },
        "useBuiltIns": "usage",
        "modules": false,
        "loose": true
      }
    ],
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  }
}
