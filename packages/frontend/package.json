{
  "name": "quiet",
  "productName": "Quiet",
  "author": {
    "name": "Zbay LLC",
    "email": "h@zbay.llc"
  },
  "license": "ISC",
  "build": {
    "afterSign": "scripts/notarize.js",
    "afterAllArtifactBuild": "scripts/setEnvs.js",
    "productName": "Quiet",
    "appId": "Quiet",
    "protocols": {
      "name": "electron-deep-linking",
      "schemes": [
        "quiet"
      ]
    },
    "mac": {
      "icon": "./build/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "category": "public.app-category.social-networking",
      "files": [
        "dist/**/*",
        "build/icon.*",
        "package.json"
      ]
    },
    "win": {
      "icon": "./build/icon.png",
      "target": "NSIS",
      "sign": "scripts/sign.js",
      "files": [
        "dist/**/*",
        "build/icon.*",
        "package.json"
      ]
    },
    "linux": {
      "icon": "./build/icon.png",
      "target": [
        "AppImage"
      ],
      "extraFiles": [
        {
          "from": "tor/libssl.so",
          "to": "usr/lib/libssl.so"
        }
      ],
      "files": [
        "dist/**/*",
        "build/icon.*",
        "package.json"
      ]
    },
    "publish": {
      "provider": "s3",
      "bucket": "test.quiet"
    },
    "extraResources": [
      "worker/**/*",
      "tor/**/*",
      "tlgManager/**/*"
    ]
  },
  "homepage": "https://github.com/ZbayApp",
  "@comment version": "To build new version for specific platform, just replace platform in version tag to one of following linux, mac, windows",
  "version": "0.3.0-alpha.1",
  "description": "Decentralized team chat",
  "main": "dist/main/main.js",
  "scripts": {
    "copyBinaries": "cp -R ../../3rd-party/tor/$SOURCE_PATH/ ./tor/ && chmod 775 ./tor/tor",
    "copyBinariesWin": "xcopy ..\\..\\3rd-party\\tor\\win32 .\\tor\\",
    "pullBinaries": "wget -N https://zbay-binaries.s3.us-east-2.amazonaws.com/$SOURCE_PATH/tor -P ./tor/ && chmod 775 ./tor/tor",
    "pullLibs": "wget -N https://zbay-binaries.s3.us-east-2.amazonaws.com/$SOURCE_PATH/libssl.so -P ./ && chmod 775 ./libssl.so",
    "pullBinariesWin": "wget -N https://zbay-binaries.s3.us-east-2.amazonaws.com/$SOURCE_PATH/tor.exe -P ./tor/  && chmod 775 ./tor/tor.exe",
    "dist": "export SOURCE_PATH=darwin && npm run copyBinaries && npm run build:prod && electron-builder --mac",
    "distUbuntu": "export SOURCE_PATH=linux && npm run copyBinaries && npm run pullLibs && npm run build:prod && electron-builder --linux",
    "distwin": "export SOURCE_PATH=win32 && npm run copyBinariesWin && npm run build:prod && electron-builder --win",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx ./src/ --fix",
    "lint-ci": "eslint --ext .jsx,.js,.ts,.tsx ./src/",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "rtl-test": "jest ./src/rtl-tests",
    "prepublish": "npm run build:main",
    "build:prod": "concurrently \"npm run build:main\" \"npm run build:renderer:prod\"",
    "build:dev": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:renderer": "webpack --config webpack/webpack.config.renderer.dev.js",
    "build:main": "tsc -p tsconfig.build.json",
    "build:renderer:prod": "webpack --config webpack/webpack.config.renderer.prod.js",
    "prestart": "npm run build:main",
    "start": "cross-env DEBUG='waggle:*,quiet:*,nectar:*' npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron .",
    "start:renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.renderer.dev.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --exit-zero-on-changes=false",
    "rmDist": "rm -rf dist/"
  },
