{
  "name": "helia",
  "version": "4.0.2",
  "description": "An implementation of IPFS in JavaScript",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/helia/tree/main/packages/helia#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/helia.git"
  },
  "private": true,
  "type": "module",
  "bugs": {
    "url": "https://github.com/ipfs/helia/issues"
  },
  "keywords": [
    "IPFS"
  ],
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "!lib/test"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "lint:no-fix": "eslint --ext .ts .",
    "lint": "npm run lint:no-fix -- --fix",
    "lint-ci": "npm run lint:no-fix",
    "lint-staged": "lint-staged --no-stash"
  },
  "dependencies": {
    "@helia/block-brokers": "2.0.3",
    "@helia/interface": "^4.0.1",
    "@helia/routers": "^1.0.1",
    "@helia/utils": "^0.0.2",
    "@libp2p/interface": "^1.7.0",
    "@libp2p/keychain": "4.1.6",
    "interface-blockstore": "^5.2.10",
    "interface-datastore": "^8.2.11",
    "libp2p": "^1.7.0",
    "multiformats": "13.1.0"
  },
  "devDependencies": {
    "@quiet/eslint-config": "^2.0.2-alpha.0",
    "lint-staged": "^15.2.2"
  },
  "lint-staged": {
    "*.{ts}": "npm run lint:no-fix"
  }
}
